<table>
  
  <table class="table table-bordered">
    <thead class="thead-dark">
      
       <th>Symbol</th>
       <th>High</th>
       <th>Low</th>
       <th>24h Average</th>

     </thead>
     <tr>
            
            <% if @bitcoin %>
              <td>Bitcoin</td>
              <td><%= @bitcoin['high'] %></td>
              <td><%= @bitcoin['low'] %></td>
              <td><%= @bitcoin['average'] %></td>
            <% else %>
              <td>There was an error retrieving data</td>
              <td><%= link_to 'Back', :back, class: 'btn btn-sm btn-primary' %></td>
              <td>&nbsp;</td>
              <td>&nbsp;</td>
              <td>&nbsp;</td>
              <td>&nbsp;</td>
            <% end %>
            </tr>
            
</table>